var x = 200;
var y = 200;

var headW = 40;
var headCW = headW - 5;
var headY = y - 100;
var headCY = headY - 55;

noStroke();
fill(0);

beginShape();
vertex(x - headW, headY);
bezierVertex(x - headCW, headCY, x + headCW, headCY, x + headW, headY);
bezierVertex(x + 100, y - 63, x + 180, y - 30, x + 80, y - 10);
endShape();
