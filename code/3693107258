        <script>
        // Randomize the <li>s
        var lists = document.getElementsByTagName("ul");
        for (var j = 0; j < lists.length; j++) {
            var list = lists[j];
            for (var i = list.children.length; i >= 0; i--) {
                list.appendChild(list.children[Math.random() * i | 0]);
            }
        }
        </script>
