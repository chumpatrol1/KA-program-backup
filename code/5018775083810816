// Change these values
var x1 = 91;
var y1 = 174;
var w1 = 80;
var h1 = 80;

var x2 = 160;
var y2 = 231;
var w2 = 80;
var h2 = 80;

strokeWeight(2);
stroke(0);

// The actual test
if (x1 + w1 >= x2 && x1 <= x2 + w2 && y1 + h1 >= y2 && y1 <= y2 + h2) {
        fill(255, 0, 0, 100);
} else {
    noFill();    
}

rect(x1, y1, w1, h1);
rect(x2, y2, w2, h2);


