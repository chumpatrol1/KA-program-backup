var moleculeCount = 0;
for (var m in molecules) {
    moleculeCount++;
}
//println(moleculeCount);

// Return the difference of the z-coordinate
var sortByZ = function(a, b) {
    return a.z - b.z;
};

// Array of to store data for a selected molecule
var atoms = [];
var bonds = [];

// Ensure molecule is centred
var moveObjectToCentre = function(atoms) {
    var dims = ['x', 'y', 'z'];
    
    for (var d = 0; d < 3; d++) {
        // Find mean values for each dimension
        var meanValue = 0;
        var dimension = dims[d];
        for (var i = 0; i < atoms.length; i++) {
            meanValue += atoms[i][dimension];
        }
        meanValue /= atoms.length;
        
        for (i=0; i < atoms.length; i++) {
            atoms[i][dimension] = scaleValue * (atoms[i][dimension] - meanValue);
        }
    }
};

// Given a molecule object, return a list of atoms and a list of bond objects.
var loadMolecule = function(molecule) {
    
    // Load atoms
    atoms = [];
    for (var i = 0; i < molecule.atoms.length; i++) {
        var atom = molecule.atoms[i];
        atoms.push({
            x: atom[1],
            y: atom[2],
            z: atom[3],
            size: atomData[atom[0]].size,
            colour: atomData[atom[0]].colour,
        });
    }
    
    // Load bonds
    bonds = [];
    for (var i = 0; i < molecule.bonds.length; i++) {
        bonds.push([
            atoms[molecule.bonds[i][0]],
            atoms[molecule.bonds[i][1]],
        ]);
    }
    
    moveObjectToCentre(atoms);
    atoms.sort(sortByZ);
};

/**************************************************************
 *       Rotation Transformations
**************************************************************/

var rotateY3D = function(theta) {
    var ct = cos(theta);
    var st = sin(theta);
    var x, z;

    for (var i = 0; i < atoms.length; i+=1) {
        x = atoms[i].x;
        z = atoms[i].z;
        atoms[i].x =  ct * x + st * z;
        atoms[i].z = -st * x + ct * z;
    }
};

var rotateX3D = function(theta){
    var ct = cos(theta);
    var st = sin(theta);
    var y, z;
    
    for (var i = 0; i < atoms.length; i+=1) {
        y = atoms[i].y;
        z = atoms[i].z;
        atoms[i].y = ct * y - st * z;
        atoms[i].z = st * y + ct * z;
    }
};


/**************************************************************
 *     Button
**************************************************************/
var menuButtons = [];
var buttons = [];

var Button = function(x, y, w, h, name, isBox) {
    this.x = x;
    this.y = y;
    this.w = w;
    this.h = h;
    this.name = name;
    this.defaultCol = color(180, 180, 180);
    this.highlightCol = color(60, 60, 250);
    
    this.drawStyle = isBox ? 'drawWithBox' : 'drawAsLink';

    this.highlight = function() {
        // Compensate for transformation
        var mx = mouseX - cx;
        var my = mouseY - cy;
        
        if (mx >= this.x && mx <= this.x + this.w && my >= this.y && my <= this.y + this.h) {
            this.mouseover = true;
        } else {
            this.mouseover = false;
        }
    };
    
    this.click = function() {
        this.selected = false;
        if (this.mouseover) {
            this.selected = true;
            if (isBox) {
                loadMolecule(molecules[this.name]);
                mode = this.name;   
            } else {
                selectedCategory = this;
            }
        }
    };
    
    this.drawWithBox = function() {
        noStroke();
        if (this.mouseover) {
            fill(this.highlightCol);
        } else {
            fill(this.defaultCol);
        }
        rect(this.x, this.y, this.w, this.h, 6);
        
        fill(10);
        if (this.name.length < 12) {
            textFont(sansFont, 13);
        } else if (this.name.length < 18) {
            textFont(sansFont, 11);
        } else {
            textFont(sansFont, 10);
        }
        textAlign(CENTER, CENTER);
        text(this.name, this.x + this.w/2, this.y + this.h/2);
    };
    
    this.drawAsLink = function() {
        noStroke();
        
        textFont(sansFont, menuFontSize);
        textAlign(LEFT, TOP);
        fill(10, 10, 20);
        
        var y = this.y + textAscent() + textDescent() + 2;
        if (this.mouseover || this.selected) {
            fill(this.highlightCol);
            stroke(this.highlightCol);
            line(this.x - 1, y, this.x + textWidth(this.name), y);
        }
        
        text(this.name, this.x, this.y);
    };
    
    this.draw = function() {
        this[this.drawStyle]();
    };
};

/**************************************************************
 *      Set-up
 *  Read molecule data and generate menu
**************************************************************/

var countMoleculesInCategory = function(arr) {
    var count = 0 ;
    for (var i = 0; i < arr.length; i++) {
        if (molecules[arr[i]]) {
            count++;   
        }
    }
    return count;
};

// Create a button for each molecule
var createButtons = function() {
    var x = 10 - cx;
    var y = 60 - cy;
    var w = 130;
    var h = 18;
    
    for (var category in categories) {
        var b = new Button(x, y, w, h + 1, category, false);
        b.mode = category;
        
        // Set up buttons for this category
        b.buttons = [];
        var moleculesInCategory = categories[category].molecules;
        var x2 = divX - cx;
        var y2 = 165 - cy;
        var w2 = 105;
        
        for (var i = 0; i < moleculesInCategory.length; i++) {
            if (molecules[moleculesInCategory[i]]) {
                var b2 = new Button(x2, y2, w2, h, moleculesInCategory[i], true);  
                b.buttons.push(b2);
                y2 += 22;
                if (y2 > 400 - cy - 20) {
                    y2 = 165 - cy;
                    x2 += w2 + 10;
                }
            }
        }
        
        b.name = category + " (" + b.buttons.length + ")";
        menuButtons.push(b);
        
        y += 25;
    }
};

// Which buttons to show and work with
var setButtons = function() {
    // Always include menu buttons
    buttons = menuButtons;
    
    if (shownCategory) {
        buttons = menuButtons.concat(shownCategory.buttons);
    }
};

createButtons();
setButtons();

/**************************************************************
 *      Draw functions
**************************************************************/
var drawMenu = function() {
    //Title
    strokeWeight(1);
    stroke(140);
    fill(BANNERCOLOUR);
    line(divX - cx - 10, -cy, divX -cx - 10, height);
    rect(-cx - 2, -cy - 2, width + 2, 52);
    
    fill(255);
    textSize(26);
    textAlign(CENTER, CENTER);
    text("Biomolecular Structures", 0, 16 - cy);
    textSize(13);
    text("Select a category", 0, 38 - cy);
    
    // Buttons
    // Selected category shows category clicked otherwise category hovered on
    
    //shownCategory = selectedCategory;
    for (var i = 0; i < buttons.length; i++) {
        var button = buttons[i];
        button.draw();
        if (!selectedCategory && button.mode && button.mouseover) {
            shownCategory = button;
        }
    }
    
    if (shownCategory) {
        if (categories[shownCategory.mode]) {
            fill(10);
            textSize(12);
            textAlign(LEFT, TOP);
            text(categories[shownCategory.mode].info, divX - cx, 60 - cy, width - divX - 10, 400);   
        }
    }
};

var drawMolecule = function() {
    // Draw bonds
    for (var i = 0; i < bonds.length; i++) {
        var atom1 = bonds[i][0];
        var atom2 = bonds[i][1];
        
        var dx = atom1.x - atom2.x;
        var dy = atom1.y - atom2.y;
        var theta = atan2(dx, dy);

        var x1 = atom1.x - 0.5 * atom1.size * sin(theta);
        var y1 = atom1.y - 0.5 * atom1.size * cos(theta);
        var x2 = atom2.x + 0.5 * atom2.size * sin(theta);
        var y2 = atom2.y + 0.5 * atom2.size * cos(theta);
        
        stroke(BOND_COLOUR);
        strokeWeight(bondThickness);
        line(x1, y1, x2, y2);
        stroke(BOND_HIGHLIGHT);
        strokeWeight(1);
        line(x1, y1, x2, y2);
    }
    
    // Draw atoms
    noStroke();
    var atom;
    for (var i = 0; i < atoms.length; i++) {
        atom = atoms[i];
        fill(atom.colour);
        ellipse(atom.x, atom.y, atom.size, atom.size);
        fill(HIGHLIGHT);
        ellipse(atom.x, atom.y, atom.size - 2, atom.size - 2);
        ellipse(atom.x - 1, atom.y - 1, atom.size - 9, atom.size - 9);
    }
};

var drawBackArrow = function(x, y) {
    noStroke();
    // TODO: Generalise coordinates
    if (mouseX < 30 && mouseY < 25) {
        fill(40, 160, 60);
    } else {
        fill(150, 150, 180);
    }
    quad(x, y + 8, x + 16, y, x + 12, y + 8, x + 16, y + 16);
};

var drawInfo = function() {
    fill(220, 220, 220, 200);
    rect(6 - cx, 4 - cy, width - 12, 90, 5);
    
    fill(0, 0, 0);
    textAlign(CENTER, TOP);
    textSize(16);
    text(mode, 0, 8 - cy);
    
    if (molecules[mode].info) {
        textSize(11);
        text(molecules[mode].info, 10 - cx, 27 - cy, width - 20, 100);   
    }
    drawBackArrow(14 - cx, 8 - cy);
};

var draw = function() {
    background(BACKGROUND);
    if (mode === 'menu') {
        drawMenu();
    } else {
        drawMolecule();
        drawInfo();
    }
};

/**************************************************************
 *      Event handling
**************************************************************/

var keyPressed = function() { 
    if (keyCode === LEFT) {
        rotateY3D(-3);
    } else if (keyCode === RIGHT) {
        rotateY3D(3);
    } else if (keyCode === UP) {
        rotateX3D(-3);
    } else if (keyCode === DOWN) {
        rotateX3D(3);
    }
    atoms.sort(sortByZ);
};

// Let go when mouse leaves canvas
var mouseOut = function(){
    mouseIsPressed = false;
    setButtons();
};

var mouseDragged = function() {
    rotateY3D(mouseX - pmouseX);
    rotateX3D(mouseY - pmouseY);
    atoms.sort(sortByZ);
};

var mouseMoved = function() {
    if  (mode === 'menu') {
        for (var b = 0; b < buttons.length; b++) {
            if (buttons[b]) {
                buttons[b].highlight();   
            }
        }
        setButtons();
    }
};

var mouseReleased = function() {
    // Click back arrow
    if  (mode === 'menu') {
        selectedCategory = false;
        for (var b = 0; b < buttons.length; b++) {
            if (buttons[b]) {
                buttons[b].click();
            }
        }
    } else {
        if (mouseX < 30 && mouseY < 25) {
            mode = 'menu';
        }
    }
    setButtons();
};
